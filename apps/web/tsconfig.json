{
  "extends": "@smartleadmagnet/typescript-config/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.json",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules","sst.config.ts"
  ]
}
